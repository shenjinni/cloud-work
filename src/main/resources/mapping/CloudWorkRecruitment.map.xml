<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
<mapper namespace="cn.testin.dao.CloudWorkRecruitmentMapper">
	<resultMap id="CloudWorkRecruitment" type="cn.testin.bean.CloudWorkRecruitment">
		<id property="id" column="id" jdbcType="NUMERIC"/>
		<result property="workType" column="work_type" jdbcType="VARCHAR"/>
		<result property="salary" column="salary" jdbcType="VARCHAR"/>
		<result property="company" column="company" jdbcType="VARCHAR"/>
		<result property="address" column="address" jdbcType="VARCHAR"/>
		<result property="contactsName" column="contacts_name" jdbcType="VARCHAR"/>
		<result property="mobile" column="mobile" jdbcType="VARCHAR"/>
		<result property="note" column="note" jdbcType="VARCHAR"/>
		<result property="status" column="status" jdbcType="NUMERIC"/>
		<result property="createUser" column="create_user" jdbcType="NUMERIC"/>
		<result property="createTime" column="create_time" jdbcType="DATE"/>
		<result property="updateTime" column="update_time" jdbcType="DATE"/>
		<result property="updateUser" column="update_user" jdbcType="NUMERIC"/>
	</resultMap>

	<sql id="columns">
		id,work_type,salary,company,address,contacts_name,mobile,note,status,create_user,create_time,update_time,update_user
	</sql>

	<sql id="dynamicWhere">
		<where>
			<if test="@Ognl@isNotEmpty(id)"> AND id  =#{id} </if>
			<if test="@Ognl@isNotEmpty(workType)"> AND work_type  LIKE #{workType}  </if>
			<if test="@Ognl@isNotEmpty(salary)"> AND salary  LIKE #{salary}  </if>
			<if test="@Ognl@isNotEmpty(company)"> AND company  LIKE #{company}  </if>
			<if test="@Ognl@isNotEmpty(address)"> AND address  LIKE #{address}  </if>
			<if test="@Ognl@isNotEmpty(contactsName)"> AND contacts_name  LIKE #{contactsName}  </if>
			<if test="@Ognl@isNotEmpty(mobile)"> AND mobile  LIKE #{mobile}  </if>
			<if test="@Ognl@isNotEmpty(note)"> AND note  LIKE #{note}  </if>
			<if test="@Ognl@isNotEmpty(status)"> AND status  =#{status} </if>
			<if test="@Ognl@isNotEmpty(createUser)"> AND create_user  =#{createUser} </if>
			<if test="@Ognl@isNotEmpty(createTime)"> AND create_time  =#{createTime} </if>
			<if test="@Ognl@isNotEmpty(begincreateTime)"> AND create_time  >=#{begincreateTime,jdbcType=DATE} </if>
			<if test="@Ognl@isNotEmpty(endcreateTime)"> AND create_time <![CDATA[ <=#{endcreateTime,jdbcType=DATE}]]> </if>
			<if test="@Ognl@isNotEmpty(updateTime)"> AND update_time  =#{updateTime} </if>
			<if test="@Ognl@isNotEmpty(beginupdateTime)"> AND update_time  >=#{beginupdateTime,jdbcType=DATE} </if>
			<if test="@Ognl@isNotEmpty(endupdateTime)"> AND update_time <![CDATA[ <=#{endupdateTime,jdbcType=DATE}]]> </if>
			<if test="@Ognl@isNotEmpty(updateUser)"> AND update_user  =#{updateUser} </if>
		</where>
	</sql>

	<insert id="insertSelective" parameterType="cn.testin.bean.CloudWorkRecruitment">
		INSERT INTO cloud_work_recruitment
		(id,work_type,salary,company,address,contacts_name,mobile,note,status,create_user,create_time,update_time,update_user)
		VALUES
		(#{id,jdbcType=NUMERIC}, #{workType,jdbcType=VARCHAR}, #{salary,jdbcType=VARCHAR}, #{company,jdbcType=VARCHAR}, #{address,jdbcType=VARCHAR}, #{contactsName,jdbcType=VARCHAR}, #{mobile,jdbcType=VARCHAR}, #{note,jdbcType=VARCHAR}, #{status,jdbcType=NUMERIC}, #{createUser,jdbcType=NUMERIC}, #{createTime,jdbcType=DATE}, #{updateTime,jdbcType=DATE}, #{updateUser,jdbcType=NUMERIC})
	</insert>
	
	<delete id="deleteByPrimaryKey" parameterType="java.lang.Long">
		DELETE FROM cloud_work_recruitment 
		WHERE
		id=#{id}
	</delete>
	
	<update id="updateByPrimaryKeySelective" parameterType="cn.testin.bean.CloudWorkRecruitment">
		UPDATE cloud_work_recruitment SET
		work_type=#{workType,jdbcType=VARCHAR},
		salary=#{salary,jdbcType=VARCHAR},
		company=#{company,jdbcType=VARCHAR},
		address=#{address,jdbcType=VARCHAR},
		contacts_name=#{contactsName,jdbcType=VARCHAR},
		mobile=#{mobile,jdbcType=VARCHAR},
		note=#{note,jdbcType=VARCHAR},
		status=#{status,jdbcType=NUMERIC},
		create_user=#{createUser,jdbcType=NUMERIC},
		create_time=#{createTime,jdbcType=DATE},
		update_time=#{updateTime,jdbcType=DATE},
		update_user=#{updateUser,jdbcType=NUMERIC}
		WHERE
		id=#{id}
	</update>
		    
	<select id="selectByPrimaryKey" parameterType="java.lang.Long" resultMap="CloudWorkRecruitment">
		SELECT <include refid="columns"/>
		FROM cloud_work_recruitment
		WHERE
		id=#{id}
	</select>

	<select id="selectList" resultMap="CloudWorkRecruitment" parameterType="cn.testin.bean.CloudWorkRecruitment" >
		select
		<include refid="columns" />
		from cloud_work_recruitment
		where 1=1
		<if test="workType != null  and workType != ''" >
			and work_type like concat('%',#{workType,jdbcType=VARCHAR},'%')
		</if>
		<if test="company != null  and company != ''" >
			and company like concat('%',#{company,jdbcType=VARCHAR},'%')
		</if>
		<if test="mobile != null  and mobile != ''" >
			and mobile like concat('%',#{mobile,jdbcType=VARCHAR},'%')
		</if>
		<if test="status != null  and status != ''" >
			and status = #{status,jdbcType=NUMERIC}
		</if>
		order by create_time DESC
		<if test="offset != null and limit != null ">
			limit #{offset,jdbcType=INTEGER}, #{limit,jdbcType=INTEGER}
		</if>
	</select>

	<select id="selectCount" parameterType="cn.testin.bean.CloudWorkRecruitment" resultType="int">
		SELECT
		count(*)
		FROM
		cloud_work_recruitment
		where 1=1
		<if test="workType != null  and workType != ''" >
			and work_type like concat('%',#{workType,jdbcType=VARCHAR},'%')
		</if>
		<if test="company != null  and company != ''" >
			and company like concat('%',#{company,jdbcType=VARCHAR},'%')
		</if>
		<if test="mobile != null  and mobile != ''" >
			and mobile like concat('%',#{mobile,jdbcType=VARCHAR},'%')
		</if>
		<if test="status != null  and status != ''" >
			and status = #{status,jdbcType=NUMERIC}
		</if>
	</select>
</mapper>
